PanelWidgetClass UIActionEditableRichText {
 size 1 30
 halign left_ref
 valign top_ref
 hexactpos 0
 vexactpos 0
 hexactsize 0
 vexactsize 1
 color 0 0 0 0
 style rover_sim_colorable
 scriptclass "UIActionEditableRichText"
 {
  PanelWidgetClass action_wrapper
  {
   position 0 0
   size 1 1
   valign center_ref
   {
    PanelWidgetClass action_wrapper_input
    {
     position 0 0
     size 1 1
     {
      RichTextWidgetClass action_label {
       size 1 20
       vexactsize 1
       halign left_ref
       valign center_ref
       text "{LABEL}"
       font "gui/fonts/sdf_MetronLight24"
       "text offset" 10 0
       "bold text" 1
       "exact text" 1
       "exact text size" 16
       "text valign" center
      }
      EditBoxWidgetClass action {
       position 0 -1
       size 0.7 22
       vexactpos 1
       vexactsize 1
       halign right_ref
       valign center_ref
       text "{TEXT}"
       "exact text" 1
       "no focus" 0
       font "gui/fonts/Metron"
       style Editor
      }
     }
    }
   }
  }
  PanelWidgetClass action_wrapper_disable {
   visible 0
   clipchildren 0
   ignorepointer 0
   color 0.1569 0.1569 0.1569 0.902
   position 0 0
   size 1 1
   style rover_sim_colorable
  }
 }
}